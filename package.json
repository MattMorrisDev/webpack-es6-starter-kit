{
  "name": "webpackacademy",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "yarn webpack-dev-server --env.env=dev",
    "start:prod": "yarn webpack-dev-server --env.env=prod",
    "debug": "node --inspect-brk webpack",
    "debug:dev": "node --inspect-brk webpack --env.env=dev",
    "debug:prod": "node --inspect-brk webpack --env.env=prod",
    "build": "rm -rf dist && webpack",
    "build:dev": "yarn build --env.env=dev",
    "build:prod": "yarn build --env.env=prod",
    "build:watch": "yarn build --watch",
    "build:watch:dev": "yarn build:watch --env.env=dev",
    "build:watch:prod": "yarn build:watch --env.env=prod",
    "build:log": "yarn build --env.log=1 --env.idk=2",
    "build:prod:bundle-analyzer": "yarn build:prod --env.addons=bundleanalyzer",
    "build:prod:bundle-buddy": "yarn build:prod --env.addons=bundlebuddy",
    "precommit": "lint-staged",
    "webpack-defaults": "webpack-defaults",
    "webpack-dev-server": "webpack-dev-server"
  },
  "dependencies": {},
  "devDependencies": {
    "bundle-buddy": "^0.2.1",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.13.5",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
